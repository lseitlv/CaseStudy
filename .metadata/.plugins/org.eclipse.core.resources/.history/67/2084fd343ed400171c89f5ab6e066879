package testFramework;

	import client.combined.org.openqa.selenium.WebDriver;
	import client.combined.org.openqa.selenium.firefox.FirefoxDriver;

public class TC1_WikipediaTest {

	public static void main(String[] args) {
		System.setProperty("webdriver.gecko.driver", "D:\\\\ToolsQA\\trunk\\Library\\drivers\\geckodriver.exe");
		WebDriver driver = new FirefoxDriver();
		driver.get("http://www.toolsqa.com");
		 
		//Wait for 5 Sec
		Thread.sleep(5000);
				
		// Close the driver
		driver.quit();

	}

}
